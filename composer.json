{
  "name": "based/momentum-trail",
  "description": "Fully typed frontend route helper for Laravel apps",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Boris Lepikhin",
      "email": "boris@lepikhin.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Momentum\\Trail\\": "src/",
      "Momentum\\Trail\\Tests\\": "tests/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Momentum\\Trail\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.24|^9.0|^10.0|^11.0",
    "tightenco/ziggy": "^1.5"
  },
  "require-dev": {
    "orchestra/testbench": "^6.9|^7.0|^8.0|^9.0",
    "larastan/larastan": "^1.0|^2.0",
    "pestphp/pest": "^1.22|^2.0",
    "pestphp/pest-plugin-laravel": "^1.3|^2.0",
    "laravel/pint": "^1.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Momentum\\Trail\\TrailServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
